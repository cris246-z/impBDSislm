CREATE TABLE inventario(
fecha VARCHAR (10) NOT NULL,
ajuste VARCHAR (10) NOT NULL
);
 
INSERT INTO inventario(fecha,ajuste)
VALUES ('03/01/2025', '100');

INSERT INTO inventario(fecha,ajuste)
VALUES ('03/01/2025', '100');

INSERT INTO inventario(fecha,ajuste)
VALUES ('03/01/2025', '100');

INSERT INTO inventario(fecha,ajuste)
VALUES ('03/01/2025', '100');

INSERT INTO inventario(fecha,ajuste)
VALUES ('03/01/2025', '100');

SELECT fecha,
           ajuste,
           SUM(ajuste) OVER (ORDER BY fecha) AS Inventario
     FROM inventario;
