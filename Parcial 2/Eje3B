CREATE TABLE experimento (
caso VARCHAR (1) NOT NULL
);

INSERT INTO experimento (caso )
VALUES ('A');

INSERT INTO experimento (caso )
VALUES ('B');

INSERT INTO experimento (caso )
VALUES ('C');

SELECT 
  CONCAT(A.caso, ',', B.caso, ',', C.caso) AS permutacion
FROM 
  experimento A 
  CROSS JOIN experimento B
  CROSS JOIN experimento C
WHERE 
  A.caso <> B.caso
  AND A.caso <> C.caso
  AND B.caso <> C.caso;

