CREATE TABLE DirectorioTel (
CustomerId int (4) NOT NULL,
TypePhoneNumber varchar (10)NOT NULL,
Nunber varchar(13) NOT NULL
CREATE TABLE NumerodeDispositivos (
CustomerId int NOT NULL,
Celular varchar (10),
10
Trabajo varchar (10),
11
Casa varchar (10),
12
PRINARY KEY (CustomerId)
