SELECT
17
CustomerId,
18
MAX(CASE MHEN TypePhoneNumber - 'Celular' THEN Number END) AS 'Celular',
19
MAX(CASE NHEN TypePhoneNumber-'Trabajo' THEN Number END) AS "Trabajo',
20
MAX(CASE NHEN TypePhoneNumber-"Casa'THEN Number END)AS "Casa'
21
FROM
22
DirectorioTel
23
GROUP BY
24
CustomerId;
